{
  "name": "rankflix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:ssr-server": "cross-env NODE_ENV=production node server/build.js",
    "build": "rimraf dist && npm run build:client && npm run build:ssr && npm run build:ssr-server",
    "dev:client": "vite",
    "dev": "cross-env NODE_ENV=local node server/index.js",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint .",
    "lint:fix:eslint": "eslint . --fix",
    "lint:fix:prettier": "prettier --write .",
    "lint:fix:stylelint": "stylelint '**/*.{css,scss}' --fix",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint '**/*.{css,scss}'",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui --coverage",
    "test": "vitest"
  },
  "dependencies": {
    "@fontsource-variable/reddit-sans-condensed": "^5.2.7",
    "@fontsource/poppins": "^5.2.7",
    "@fontsource/space-mono": "^5.2.9",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "compression": "1.8.1",
    "dayjs": "^1.11.18",
    "express": "5.1.0",
    "humanize-duration": "^3.33.1",
    "motion": "^12.23.24",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.3",
    "sirv": "3.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "9.37.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/compression": "^1.8.1",
    "@types/express": "5.0.3",
    "@types/humanize-duration": "^3.27.4",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react-swc": "4.1.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cross-env": "10.1.0",
    "dependency-cruiser": "^17.0.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-sort": "^4.0.0",
    "eslint-plugin-unicorn": "^61.0.2",
    "global-jsdom": "27.0.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jsdom": "27.0.0",
    "lint-staged": "16.2.3",
    "playwright": "1.56.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "sass-embedded": "1.93.2",
    "steiger": "^0.5.11",
    "stylelint": "^16.25.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vite": "7.1.9",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
